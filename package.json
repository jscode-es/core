{
	"name": "core",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"engines": {
		"node": ">=20.10.0"
	},
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"prisma:init": "prisma init --preview-feature views --datasource-provider mysql",
		"prisma:pull": "prisma db pull",
		"prisma:generate": "prisma generate"
	},
	"dependencies": {
		"@fastify/csrf-protection": "6.4.1",
		"@fastify/helmet": "11.1.1",
		"@fastify/multipart": "8.2.0",
		"@fastify/secure-session": "7.1.0",
		"@google-cloud/storage": "7.9.0",
		"@maxmind/geoip2-node": "5.0.0",
		"@nestjs/common": "10.3.6",
		"@nestjs/config": "3.2.1",
		"@nestjs/core": "10.3.6",
		"@nestjs/mapped-types": "2.0.5",
		"@nestjs/platform-express": "10.3.6",
		"@nestjs/platform-fastify": "10.3.6",
		"@nestjs/platform-socket.io": "10.3.6",
		"@nestjs/schedule": "4.0.1",
		"@nestjs/websockets": "^10.3.6",
		"@prisma/client": "5.11.0",
		"bcrypt": "5.1.1",
		"cache-manager": "5.4.0",
		"device-detector-js": "3.0.3",
		"fastify": "^4.26.2",
		"handlebars": "4.7.8",
		"hashids": "2.3.0",
		"joi": "17.12.2",
		"jsonwebtoken": "9.0.2",
		"morgan": "1.10.0",
		"nodemailer": "6.9.13",
		"rate-limiter-flexible": "5.0.0",
		"reflect-metadata": "0.2.1",
		"request-ip": "3.3.0",
		"rxjs": "7.8.1",
		"sharp": "0.33.3",
		"socket.io": "^4.7.5",
		"stripe": "14.22.0"
	},
	"devDependencies": {
		"@clack/prompts": "0.7.0",
		"@nestjs/cli": "10.3.2",
		"@nestjs/schematics": "10.1.1",
		"@nestjs/testing": "10.3.6",
		"@types/bcrypt": "5.0.2",
		"@types/express": "4.17.21",
		"@types/jest": "29.5.12",
		"@types/jsonwebtoken": "9.0.6",
		"@types/morgan": "1.9.9",
		"@types/node": "20.11.30",
		"@types/nodemailer": "6.4.14",
		"@types/request-ip": "0.0.41",
		"@types/supertest": "6.0.2",
		"@typescript-eslint/eslint-plugin": "7.4.0",
		"@typescript-eslint/parser": "7.4.0",
		"dotenv": "16.4.5",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.1.3",
		"jest": "29.7.0",
		"prettier": "3.2.5",
		"prisma": "5.11.0",
		"source-map-support": "0.5.21",
		"supertest": "6.3.4",
		"ts-jest": "29.1.2",
		"ts-loader": "9.5.1",
		"ts-node": "10.9.2",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.4.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
